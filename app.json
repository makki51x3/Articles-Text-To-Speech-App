{
  "expo": {
    "name": "interview_challenge",
    "slug": "interview_challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A ReactJS application that communicates with a blogging API to allow users to sign in, view, and filter different articles in a user friendly manner.",
    "githubUrl": "https://github.com/makki51x3/interview_challenge/"
  }
}
